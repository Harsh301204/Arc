<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#805BED9F"
        tools:context=".FragmentActivity">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/horizontalGuideline1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.1" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/horizontalGuideline2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.3" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/horizontalGuideline2_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.35" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/horizontalGuideline2_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.4" />



        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/horizontalGuideline3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.6" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/horizontalGuideline3_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.65" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/horizontalGuideline3_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.7" />



        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/horizontalGuideline4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.9" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/horizontalGuideline4_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.95" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/horizontalGuideline4_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="1" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/verticalGuideline1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="Our Team"
            android:textSize="35dp"
            android:textAlignment="center"
            android:gravity="bottom"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:src="@drawable/avinash"
            android:layout_margin="10dp"
            app:layout_constraintEnd_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline1"
            app:layout_constraintBottom_toBottomOf="@id/horizontalGuideline2"/>

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:src="@drawable/verma"
            app:layout_constraintStart_toEndOf="@+id/imageView4"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="@id/horizontalGuideline2"/>

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:src="@drawable/rishi"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline3"
            app:layout_constraintEnd_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline2_2" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:src="@drawable/rehmani"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="@id/horizontalGuideline3"
            app:layout_constraintStart_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline2_2" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Avinash Nishad"
            android:gravity="center"
            android:textSize="18dp"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline2_1"
            app:layout_constraintEnd_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline2" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Aditya Verma"
            android:textSize="18dp"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline2_1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline2" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:gravity="top"
            android:text="IOT and Web Developer"
            android:textColor="@color/white"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline2_2"
            app:layout_constraintEnd_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline2_1" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:gravity="top"
            android:textColor="@color/white"
            android:text="App Developer"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline2_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline2_1" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rishi Raj"
            android:textSize="18dp"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline3_1"
            app:layout_constraintEnd_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView6" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:gravity="top"
            android:text="AppDeveloper"
            android:textColor="@color/white"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline3_2"
            app:layout_constraintEnd_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline3_1" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sharique Rahmani"
            android:textSize="18dp"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline3_1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline3" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:gravity="top"
            android:text="UI/UX Designer"
            android:textColor="@color/white"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline3_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline3_1" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Aarju"
            android:textSize="18dp"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline4_1"
            app:layout_constraintEnd_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline4" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nilkamal"
            android:textSize="18dp"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline4_1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline4" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:gravity="top"
            android:textColor="@color/white"
            android:text="UI/UX Designer"
            app:layout_constraintBottom_toTopOf="@+id/horizontalGuideline4_2"
            app:layout_constraintEnd_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline4_1" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:gravity="top"
            android:text="Web Developer"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline4_1" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:src="@drawable/aarju"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="@id/verticalGuideline1"
            app:layout_constraintBottom_toTopOf="@id/horizontalGuideline4"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline3_2"
            tools:layout_editor_absoluteX="76dp" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:src="@drawable/nil"
            app:layout_constraintBottom_toTopOf="@id/horizontalGuideline4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/verticalGuideline1"
            app:layout_constraintTop_toTopOf="@+id/horizontalGuideline3_2" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>